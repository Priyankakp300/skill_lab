pipeline {
    agent any

    stages{
       stage('Checkout') {
          steps {
             git branch:'main', url:'https://github.com/Priyankakp300/skill_lab.git'
          }
       }
        stage('Heloo') {
             steps {
                echo "Hello from Jenkins Pipeline!"
          } 
       }
       stage('Goodbye') {
           steps {
               echo "Pipeline finishes successfully"
           }
        }
     }
   }
